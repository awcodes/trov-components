{
    "name": "awcodes/trov-components",
    "type": "project",
    "description": "A set of components / fields to extend Filament Admin.",
    "keywords": [
        "framework",
        "laravel",
        "filament",
        "trov",
        "forms",
        "tables"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "TrovComponents\\": "src/",
            "TrovComponents\\Database\\Factories\\": "database/factories/",
            "TrovComponents\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TrovComponents\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TrovComponents\\TrovComponentsServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}